pipeline {
    agent any
    tools{
        maven 'mymaven'
    }
    stages {
        stage('Build') {
            steps {
                echo "This the build"
                sh 'mvm clean package'
            }
            post{
                success{
                    archiveArtifacts artifact : '**/target/ *.war'
                }
            }
        }
        
            
    }
        
    
}
