pipeline {
    agent any
    tools{
        maven 'mymaven'
    }
    stages {
        stage('Build') {
            steps {
                echo "This the build"
                sh 'mvn clean package'
            }
            post{
                success{
                    archiveArtifacts artifacts : '**/target/*.jar'
                }
            }
        }
        
            
    }
        
    
}
